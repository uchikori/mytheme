					"size": "10px",
					"slug": "30"
				},
				{
					"name": "2",
					"size": "clamp(1rem, 0.8846rem + 0.2404vw, 1.125rem)",
					"slug": "40"
				},
				{
					"name": "3",
					"size": "clamp(1.25rem, 0.6731rem + 1.2019vw, 1.875rem)",
					"slug": "50"
				},
				{
					"name": "4",
					"size": "clamp(1.875rem, 0.7212rem + 2.4038vw, 3.125rem)",
					"slug": "60"
				},
				{
					"name": "5",
					"size": "clamp(2.5rem, 0.7692rem + 3.6058vw, 4.375rem)",
					"slug": "70"
				},
				{
					"name": "6",
					"size": "clamp(3.75rem, 1.4423rem + 4.8077vw, 6.25rem)",
					"slug": "80"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Noto Sans JP",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/NotoSansJP-Medium.woff2"
							]
						},
						{
							"fontFamily": "Noto Sans JP",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/NotoSansJP-Bold.woff2"
							]
						}
					],
					"fontFamily": "Noto Sans JP",
					"name": "Noto Sans JP",
					"slug": "Noto Sans JP"
				},
				{
					"fontFace": [
						{
							"fontFamily": "\"Josefin Sans\"",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/Qw3PZQNVED7rKGKxtqIqX5E-AVSJrOCfjY46_GbQXMRrLgTsQV0.woff2"
							]
						},
						{
							"fontFamily": "\"Josefin Sans\"",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/Qw3PZQNVED7rKGKxtqIqX5E-AVSJrOCfjY46_N_XXMRrLgTsQV0.woff2"
							]
						}
					],
					"fontFamily": "\"Josefin Sans\", sans-serif",
					"name": "Josefin Sans",
					"slug": "josefin-sans"
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"size": "13px",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "18px",
						"min": "16px"
					},
					"size": "18px",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "32px",
						"min": "22px"
					},
					"size": "32px",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "64px",
						"min": "36px"
					},
					"size": "64px",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "120px",
						"min": "60px"
					},
					"size": "120px",
					"slug": "xx--large"
				},
				{
					"fluid": {
						"max": "180px",
						"min": "98px"
					},
					"size": "180px",
					"slug": "ディスプレイ"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/image": {
				"border": {
					"radius": "5px"
				}
			},
			"core/post-content": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--secondary)"
						}
					}
				}
			},
			"core/post-featured-image": {
				"elements": {
					"link": {
						":hover": {
							"shadow": "0px 3px 6px rgba(0,0,0,0.4)"
						}
					}
				}
			},
			"core/post-terms": {
				"elements": {
					"link": {
						":hover": {
							"color": {
								"background": "var(--wp--preset--color--primary)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						"border": {
							"color": "var(--wp--preset--color--tertiary)",
							"radius": "10px",
							"style": "solid",
							"width": "2px"
						},
						"spacing": {
							"padding": {
								"bottom": "var(--wp--preset--spacing--30)",
								"left": "var(--wp--preset--spacing--40)",
								"right": "var(--wp--preset--spacing--40)",
								"top": "var(--wp--preset--spacing--30)"
							}
						},
						"typography": {
							"fontSize": "var(--wp--preset--font-size--small)",
							"fontStyle": "normal",
							"fontWeight": "700"
						}
					}
				}
			},
			"core/query-pagination-numbers": {
				"color": {
					"text": "var(--wp--preset--color--gray)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--josefin-sans)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontStyle": "normal",
					"fontWeight": "300"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"button": {
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--contrast)",
						"text": "var(--wp--preset--color--base)"
					},
					"shadow": "0px 0px 6px rgba(255,255,255,0.75)"
				},
				"border": {
					"radius": "10px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"fontStyle": "normal",
					"fontWeight": "700"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontStyle": "normal",
					"fontWeight": "300"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--josefin-sans)",
					"lineHeight": "1.3"
				}
			},
			"link": {
				":hover": {
					"typography": {
						"textDecoration": "underline"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"textDecoration": "none"
				}
			}
		},
		"spacing": {
			"blockGap": "1.8em",
			"padding": {
				"bottom": "var(--wp--preset--spacing--40)",
				"left": "var(--wp--preset--spacing--50)",
				"right": "var(--wp--preset--spacing--50)",
				"top": "var(--wp--preset--spacing--40)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system-font)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontStyle": "normal",
			"fontWeight": "400",
			"lineHeight": "1.8"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "サイトヘッダー"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "サイトフッター"
		},
		{
			"area": "uncategorized",
			"name": "posts"
		},
		{
			"area": "uncategorized",
			"name": "search"
		}
	],
	"version": 2,
	"$schema": "https://schemas.wp.org/wp/6.5/theme.json"
}